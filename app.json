{
  "expo": {
    "name": "FirstProgram",
    "slug": "firstprogram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "android": {
        "permissions": ["CAMERA"]
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.FirstProgram"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.FirstProgram"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "53509050-cde5-4342-bddf-65c800d5edff"
      }
    },
    "owner": "muhammad_touseef"
  }
}
